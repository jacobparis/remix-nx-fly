{
  "dependencies": {
    "@npmcli/package-json": "2.0.0",
    "@prisma/client": "4.6.1",
    "@remix-run/node": "1.7.5",
    "@remix-run/react": "1.7.5",
    "@remix-run/serve": "1.7.5",
    "@remix-run/server-runtime": "1.7.5",
    "bcryptjs": "2.4.3",
    "isbot": "3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semver": "7.3.4",
    "tailwindcss": "3.2.3",
    "tiny-invariant": "1.3.1",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nrwl/cli": "15.0.13",
    "@nrwl/eslint-plugin-nx": "15.0.13",
    "@nrwl/js": "15.0.13",
    "@nrwl/linter": "15.0.13",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.0.13",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/express-prometheus-middleware": "^1.2.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "nx": "15.0.13",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.5.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.5"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "main": "./deps.js",
  "name": "indie-stack-template",
  "peerDependencies": {},
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "npx @remix-run/dev build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "npx prettier --write .",
    "generate:css": "npx tailwindcss -o ./app/styles/tailwind.css",
    "lint": "npx eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "pretest:e2e:run": "npm run build",
    "setup": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed",
    "start": "npx @remix-run/serve build",
    "start:mocks": "npx binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "npx vitest",
    "test:e2e:dev": "npx start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "test:e2e:run": "npx cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "sideEffects": false,
  "types": "./deps.d.ts"
}
